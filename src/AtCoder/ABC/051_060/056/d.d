void times(alias fun)(int n) {
    foreach(i; 0..n) fun();
}
auto rep(alias fun, T = typeof(fun()))(int n) {
    T[] res = new T[n];
    foreach(ref e; res) e = fun();
    return res;
}
void main() {
    int N, K;
    readf("%d %d\n", &N, &K);
    int[] ary = readln.split.to!(int[]);

    bool[][] dp1 = new bool[][](N+1, K);
    dp1[0][0] = true;
    foreach(i, a; ary) {
        dp1[i+1][] = dp1[i][];
        foreach(j; 0..K-a) {
            dp1[i+1][j+a] |= dp1[i][j];
        }
    }
    bool[][] dp2 = new bool[][](N+1, K);
    dp2[0][0] = true;
    foreach(i, a; ary.retro.array) {
        dp2[i+1][] = dp2[i][];
        foreach(j; 0..K-a) {
            dp2[i+1][j+a] |= dp2[i][j];
        }
    }

    int ans = N;
    foreach(i, a; ary) {
        bool[] po1 = dp1[i];
        bool[] po2 = dp2[N-i-1];

        int l = 0;
        int r = K-a;
        int s = po2[max(0, r)..$].count!"a".to!int;
        while(l<K) {
            if (po1[l] && s>0) {
                ans--;
                break;
            }
            l++; r--;
            s -= (r+a<K && po2[r+a]) ? 1:0;
            s += (r>=0 && po2[r]) ? 1:0;
        }
    }
    ans.writeln;
}


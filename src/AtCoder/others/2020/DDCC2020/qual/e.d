enum long INF = long.max/5;
enum long MOD = 10L^^9+7;

void main() {
  long N;
  scanln(N);

  if (N == 1) {
    if (query([0])) {
      answer([true, false]);
    } else {
      answer([false, true]);
    }
    return;
  }

  long[] bs = new long[N];
  bool f(long i) {
    foreach(j; 0..N) {
      bs[j] = (i+j)%(2*N);
    }
    return query(bs);
  }

  long l = 0;
  long r = N;

  bool x = f(l);
  assert(f(r) != x);

  while(r-l>1) {
    long c = (l+r)/2;
    if (f(c) != x) {
      r = c;
    } else {
      l = c;
    }
  }

  long offset = l + 1;

  long[] bs1 = new long[N];
  long[] bs2 = new long[N];
  foreach(i; 0..N-1) {
    bs1[i] = (offset + i)%(2*N);
    bs2[i] = (offset + i + N)%(2*N);
  }

  bool[] ans = new bool[2*N];
  foreach(i; 0..2*N) {
    if (bs1[0..N-1].any!(a => a==i)) {
      bs2[N-1] = i;
      ans[i] = query(bs2);
    } else {
      bs1[N-1] = i;
      ans[i] = query(bs1);
    }
  }
  answer(ans);
}

bool query(long[] as) {
  writeln("? ", as.map!"a+1".map!(to!string).join(" "));
  stdout.flush;
  return readln.chomp == "Blue";
}

void answer(bool[] as) {
  writeln("! ", as.map!"a?'B':'R'".to!string);
}


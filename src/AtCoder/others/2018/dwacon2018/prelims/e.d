const long INF = long.max/3;
const long MOD = 10L^^9+7;

void main() {
    int N, Q;
    scanln(N, Q);

    Vertex[] vs = N.iota.map!(i => new Vertex(i)).array;
    foreach(_; 0..N-1) {
        int a, b;
        scanln(a, b);
        a--; b--;
        vs[a].edges ~= Edge(vs[a], vs[b]);
        vs[b].edges ~= Edge(vs[b], vs[a]);

        // !!!!
        if (vs[a].edges.length > 2) return;
        if (vs[b].edges.length > 2) return;
        // !!!!
    }

    Vertex[] us = new Vertex[N];
    us[0] = vs.find!"a.edges.length == 1".front;
    foreach(i; 1..N) {
        if (i==1) {
            us[i] = us[i-1].edges[0].end;
        } else {
            us[i] = us[i-1].edges.map!"a.end".find!(u => u !is us[i-2]).front;
        }
    }
    int l = 0;
    int r = N;
    while(r-l>1) {
        int ans = query(us[l].index, us[r-1].index);
        if (ans == us[l].index) {
            r = (l + r)/2;
        } else {
            l = (l + r)/2;
        }
    }
    "! %d".format(us[l].index + 1).writeln;
}

class Vertex {
    int index;
    Edge[] edges;
    this(int index) {
        this.index = index;
    }
}

struct Edge {
    Vertex start, end;
}



int query(int u, int v) {
    "? %d %d".format(u+1, v+1).writeln;
    stdout.flush;
    int ans;
    scanln(ans);
    return ans<0 ? u : ans - 1;
}


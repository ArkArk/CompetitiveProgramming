void times(alias fun)(int n) {
    foreach(i; 0..n) fun();
}
auto rep(alias fun, T = typeof(fun()))(int n) {
    T[] res = new T[n];
    foreach(ref e; res) e = fun();
    return res;
}

int[] dx = [-1, 0, 1, 0];
int[] dy = [0, -1, 0, 1];
int H, W;
struct Pos{int y, x;}
Pos s, g;
void main() {
    readf("%d %d\n", &H, &W);

    bool[][] used = H.iota.map!(
        i => readln.chomp.enumerate.map!((a) {
            int j = a.index.to!int;
            char c = a.value.to!char;
            if (c=='#') return true;
            if (c=='s') s = Pos(i, j);
            if (c=='g') g = Pos(i, j);
            return false;
        }).array
    ).array;

    writeln(rec(s, used) ? "Yes" : "No");
}

bool rec(Pos p, bool[][] used) {
    if (p == g) return true;
    used[p.y][p.x] = true;
    foreach(k; 0..4) {
        Pos q = Pos(p.y+dy[k], p.x+dx[k]);
        if (q.y<0 || q.y>=H) continue;
        if (q.x<0 || q.x>=W) continue;
        if (used[q.y][q.x]) continue;
        if (rec(q, used)) return true;
    }
    return false;
}

